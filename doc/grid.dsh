		circle 30 20 2
		circle 90 80 2
		circle 50 50 2
		circle 20 70 2
		circle 50 90 2
		circle 60 70 2
		circle 60 70 10 "black" 20
		
		ctext "(30,20)" 30 13 3
		ctext "(90,80)" 90 73 3
		ctext "(50,50)" 50 43 3
		ctext "(20,70)" 20 63 3
		ctext "(60,70)" 60 65 3
		hline  70 30 20 1
		ctext "len=20" 80 40 3
		ubrace 80 37 20 2 2
		ctext "w=10" 60 82 3
		ubrace 60 82 10 2 2
	eslide
