		ctext "60" 60 5 3 "mono"
		ctext "70" 70 5 3 "mono"
		ctext "80" 80 5 3 "mono"
		ctext "90" 90 5 3 "mono"

		ctext "10" 5 10 3 "mono"
		ctext "20" 5 20 3 "mono"
		ctext "30" 5 30 3 "mono"
		ctext "40" 5 40 3 "mono"
		ctext "50" 5 50 3 "mono"
